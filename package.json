{
  "name": "@braw/vue-intl-controller",
  "version": "1.0.0",
  "description": "Integrate @formatjs/intl into your Vue apps for easier internationalisation",
  "keywords": [
    "i18n",
    "vue3"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./types/arguments": {
      "types": "./dist/types/arguments.d.ts",
      "import": "./dist/types/arguments.mjs"
    },
    "./events": {
      "types": "./dist/events/index.d.ts",
      "import": "./dist/events/index.mjs"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.mjs"
    },
    "./controller": {
      "types": "./dist/controller.d.ts",
      "import": "./dist/controller.mjs"
    },
    "./translateFunction": {
      "types": "./dist/translateFunction.d.ts",
      "import": "./dist/translateFunction.mjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.mjs"
    },
    "./sources/*": {
      "types": "./dist/sources/*.d.ts",
      "import": "./dist/sources/*.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:raw": "del-cli dist && tsc --noEmit false",
    "build": "unbuild",
    "dev": "vitest dev",
    "prepack": "pnpm run build",
    "test": "vitest run",
    "lint": "eslint src",
    "bt": "pnpm build && pnpm test",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^18.11.18",
    "del-cli": "^5.0.0",
    "eslint": "^8.31.0",
    "happy-dom": "^8.1.5",
    "prettier": "^2.8.3",
    "prettier-plugin-jsdoc": "^0.4.2",
    "semantic-release": "^20.0.2",
    "typescript": "^4.9.4",
    "unbuild": "^1.1.1",
    "vitepress": "1.0.0-alpha.47",
    "vitest": "^0.27.2"
  },
  "dependencies": {
    "@braw/async-computed": "^5.0.1",
    "@formatjs/icu-messageformat-parser": "^2.1.14",
    "@formatjs/intl": "^2.6.3",
    "@formatjs/intl-localematcher": "^0.2.32",
    "intl-messageformat": "^10.2.5",
    "vue": "^3.2.45"
  },
  "publishConfig": {
    "access": "public"
  }
}
